%(a)
subplot(1,2,1);
W1 = 0:0.1:100;
B1 = 8;
A1 = [1 4 8 8];
H1 = freqs(B1,A1,W1);
semilogx(W,20*log10(abs(H1)))
xlabel('omega');
ylabel('Magnitude in dB');
axis tight;

